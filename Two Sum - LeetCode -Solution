class Solution {


public int[] twoSum(int[] nums, int target) {
  for (int i = 0; i < nums.length; i++) {
    for (int j = i + 1; j < nums.length; j++) { // Start j from i + 1 to avoid duplicates
      if (nums[i] + nums[j] == target) {
        return new int[] {i, j};
      }
    }
  }
  return new int[] {}; // No solution found
}


    
}
