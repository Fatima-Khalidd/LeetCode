class Solution {
    public boolean isPalindrome(int x) {
        int reverse=0;
        int org_x=x;
        if(x<0)
        {
            return false;
        }
    while(x!=0)
    {
        reverse=reverse*10+x%10;
        x=x/10;
    }
    if(reverse==org_x)
    {
        return true;
    }
    else
    return false;
    }
  
}
